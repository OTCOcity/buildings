<div class="breadcrumbs" id="breadcrumbs">

    <div class="lang__cont">
        {{ misc.langNav | raw }}
    </div>


    <ul class="breadcrumb">
        <li>

            <a href="/admin/threads/1"><i class="ace-icon fa fa-home home-icon"></i></a>
        </li>

        {% for item in items %}
            {% if not loop.last %}
                <li>
                    <a href="{{ item.url }}">{{ item.name }}</a>
                </li>
            {% else %}
                <li class="active">{{ item.name }}</li>
            {% endif %}
        {% endfor %}
    </ul><!-- /.breadcrumb -->

    <!-- #section:basics/content.searchbox -->
